<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uas_pmi18n">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="UAS Majdi Firman"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UAS_PMi18n">
        <activity
            android:name=".ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.uas_pmi18n.DaftarHewanActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity android:name="com.example.uas_pmi18n.ProfilActivity"

            />
    </application>

</manifest>